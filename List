    # Remove Duplicates
    
    
    n=int(input())
l=[]
for i in range(n):
    a=int(input())
    if a not in l:
        l.append(a)
print(l)


#Second Largest Number


n=int(input())
l=[]
s=[]
max=-9999
for i in range(n):
    a=int(input())
    
    
    #Swap the numbers
    
    
    n=int(input())
l=[]
for i in range(n):
    a=int(input())
    l.append(a)
temp=l[0]
l[0]=l[n-1]
l[n-1]=temp
print(l)


